{
    "version": 2,
    "builds": [
        {
            "src": "front/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "back/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "back/src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "front/dist/$1"
        }
    ],
    "functions": {
        "back/app.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}